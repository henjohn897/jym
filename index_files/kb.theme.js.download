(function(jQuery,Drupal){
  Drupal.behaviors.kbTheme = {
    attach: function(context,settings) {

      jQuery(document).ready(function(){

        //var autocomplete = Drupal.SearchApiAutocomplete;

        var searchApiElements = jQuery("input[data-drupal-selector='edit-search']");

        if(searchApiElements.length > 0) {

          jQuery.each(searchApiElements,function(i, elem){

            if(jQuery(jQuery(elem).closest('.kb-nav-search')).length > 0)
            {
              jQuery(elem).addClass('is-small')
              jQuery(elem).autocomplete( "option", "position", { my : "right top", at: "right bottom" } );
            }
            else {
              jQuery(elem).autocomplete( "option", "position", { my : "center top", at: "center bottom" } );
            }

            jQuery(elem).focus( function() {
              Drupal.behaviors.kbOverlay.showOverlay();
            });

            jQuery(elem).focusout( function() {
              Drupal.behaviors.kbOverlay.hideOverlay();
            });

            // jQuery(elem).on( "autocompleteopen", function( event, ui ) {
            //  console.log('autocompleteopen');
            // });
          })
        }

        var kbNavbar = jQuery(".kb-d .kb-navbar");

        if(kbNavbar) {
          jQuery(window).scroll(function(){
            var scroll = jQuery(window).scrollTop();
            if(scroll >= 40) {
              kbNavbar.addClass('is-scrolling')
            }
            else {
              kbNavbar.removeClass('is-scrolling')
            }
          })
        }

      });
    }
  }

})(jQuery,Drupal)

}
